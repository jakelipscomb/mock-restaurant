<h1>Your Cart</h1>
  <div class="cart-items">
    {{#each cartItems}}
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">{{productName}}</h5>
          <p class="card-text">
            <strong>Quantity:</strong> {{quantity}}<br>
            <strong>Price:</strong> {{price}}
          </p>
        </div>
      </div>
    {{/each}}
  </div>